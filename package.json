{
	"name": "trackhammer",
	"version": 1.0,
	"scripts": {
		"server": "npm run start --prefix server",
		"client": "npm run start --prefix client",
		"start": "concurrently \"npm run server\" \"npm run client\"",
		"install-all": "npm install --prefix server && npm install --prefix client"
	},
	"devDependencies": {
		"concurrently": "^9.1.2"
	}
}
